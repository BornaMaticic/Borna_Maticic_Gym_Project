<h1>Bookings</h1>



<p class="btn--wrapper">
  <a class="btn btn--action" href="/bookings/new">New booking</a>
</p>


  <% for booking in @bookings %>
  
  <p><%= booking.member.format_name %> is a member of <%= booking.session.session_name %> class </p>
    <p>
      <form action="/bookings/<%= booking.id %>/delete" method="POST">
        <input class="btn btn--danger" type="submit" value="delete" />
      </form>
    </p>

  <% end %>
